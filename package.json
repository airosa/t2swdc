{
  "name": "t2swdc",
  "version": "0.0.0-semantically-released",
  "description": "An SDMX Web Data Connector for Tableau",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --display-chunks --display-reasons --progress --color -p",
    "start": "webpack-dev-server --content-base dist/",
    "prebuild": "rm -rf dist && mkdir dist",
    "commit": "git-cz",
    "compile": "coffee -c -m -o dist src || true",
    "check-coverage": "istanbul check-coverage --statements 60 --branches 60 --functions 60 --lines 60",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha --require mocha-clean/brief --recursive --compilers coffee:coffee-script/register",
    "test:single": "istanbul cover -x **/*.test.coffee _mocha -- --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecbsdmx/t2swdc.git"
  },
  "keywords": [
    "sdmx",
    "tableau",
    "wdc",
    "web data connectors",
    "sdmx web services"
  ],
  "author": "ECB SDMX <ecbsdmx@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ecbsdmx/t2swdc/issues"
  },
  "homepage": "https://github.com/ecbsdmx/t2swdc#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "cjsx-loader": "^3.0.0",
    "codecov.io": "0.1.6",
    "coffee-coverage": "1.0.1",
    "coffee-loader": "0.7.2",
    "coffee-script": "1.10.0",
    "coffeelint": "1.15.7",
    "commitizen": "2.8.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "1.1.6",
    "enzyme": "2.2.0",
    "file-loader": "^0.8.5",
    "ghooks": "1.2.1",
    "html-webpack-plugin": "2.16.1",
    "istanbul": "0.4.3",
    "jquery": "2.2.3",
    "jsdom": "8.5.0",
    "json-loader": "0.5.4",
    "jsx-loader": "^0.13.2",
    "mocha": "2.4.5",
    "mocha-clean": "1.0.0",
    "mocha-jsdom": "1.1.0",
    "nock": "8.0.0",
    "react-addons-test-utils": "15.0.2",
    "semantic-release": "4.3.5",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
    }
  },
  "dependencies": {
    "crossfilter2": "1.3.13",
    "immutable": "3.8.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-actions": "0.9.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "sdmx-rest": "2.4.0",
    "sdmxmllib": "0.7.3"
  }
}
