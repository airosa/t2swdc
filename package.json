{
  "name": "t2swdc",
  "version": "0.0.0-semantically-released",
  "description": "An SDMX Web Data Connector for Tableau",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "browserify --standalone wdc -t coffeeify --extension=\".coffee\" src/index.coffee > dist/index.js && cp src/index.html dist/index.html && cp -R src/assets dist/",
    "commit": "git-cz",
    "compile": "coffee -c -m -o dist src || true",
    "check-coverage": "istanbul check-coverage --statements 65 --branches 60 --functions 65 --lines 65",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha --recursive --compilers coffee:coffee-script/register",
    "test:single": "istanbul cover -x **/*.test.coffee _mocha -- --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecbsdmx/t2swdc.git"
  },
  "keywords": [
    "sdmx",
    "tableau",
    "wdc",
    "web data connectors",
    "sdmx web services"
  ],
  "author": "ECB SDMX <ecbsdmx@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ecbsdmx/t2swdc/issues"
  },
  "homepage": "https://github.com/ecbsdmx/t2swdc#readme",
  "devDependencies": {
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "codecov.io": "0.1.6",
    "coffee-coverage": "1.0.1",
    "coffee-script": "1.10.0",
    "coffeeify": "2.0.1",
    "coffeelint": "1.15.0",
    "commitizen": "2.7.6",
    "cz-conventional-changelog": "1.1.5",
    "enzyme": "2.2.0",
    "ghooks": "1.2.1",
    "istanbul": "0.4.3",
    "jquery": "2.2.3",
    "jsdom": "8.3.1",
    "mocha": "2.4.5",
    "mocha-jsdom": "1.1.0",
    "nock": "8.0.0",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "semantic-release": "4.3.5",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
    }
  },
  "dependencies": {
    "crossfilter2": "1.3.13",
    "immutable": "3.7.6",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.4",
    "redux": "3.4.0",
    "redux-actions": "0.9.1",
    "redux-thunk": "2.0.1",
    "sdmx-rest": "2.2.0"
  }
}
