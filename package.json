{
  "name": "t2swdc",
  "version": "0.0.0-semantically-released",
  "description": "An SDMX Web Data Connector for Tableau",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "browserify --standalone wdc -t coffeeify --extension='.coffee' src/index.coffee > dist/index.js && cp src/index.html dist/index.html",
    "commit": "git-cz",
    "compile": "coffee -c -m -o dist src || true",
    "check-coverage": "istanbul check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha --recursive --compilers coffee:coffee-script/register",
    "test:single": "istanbul cover -x **/*.test.coffee _mocha -- --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "coffeelint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecbsdmx/t2swdc.git"
  },
  "keywords": [
    "sdmx",
    "tableau",
    "wdc",
    "web data connectors",
    "sdmx web services"
  ],
  "author": "ECB SDMX <ecbsdmx@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ecbsdmx/t2swdc/issues"
  },
  "homepage": "https://github.com/ecbsdmx/t2swdc#readme",
  "devDependencies": {
    "browserify": "13.0.0",
    "chai": "3.4.1",
    "codecov.io": "0.1.6",
    "coffee-coverage": "0.7.0",
    "coffee-script": "1.10.0",
    "coffeeify": "2.0.1",
    "coffeelint": "1.14.2",
    "commitizen": "2.4.6",
    "cz-conventional-changelog": "1.1.5",
    "enzyme": "1.4.1",
    "ghooks": "1.0.3",
    "istanbul": "0.4.1",
    "jsdom": "7.2.2",
    "mocha": "2.3.4",
    "react-addons-test-utils": "0.14.6",
    "react-dom": "0.14.6",
    "semantic-release": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage && npm run lint"
    }
  },
  "dependencies": {
    "immutable": "3.7.6",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "redux": "3.0.5",
    "redux-actions": "0.9.0"
  }
}
